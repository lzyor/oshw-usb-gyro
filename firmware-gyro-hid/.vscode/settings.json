    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.lst": true,
        "**/*.rst": true,
        "**/*.sym": true,
        "**/*.lk": true,
        "**/*.rel": true,
        "**/*.map": true,
        "**/.directory": true,
        "**/.gitignore": true,
        "**/.vscode": true
    }
